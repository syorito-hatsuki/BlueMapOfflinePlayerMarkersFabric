{
  "schemaVersion": 1,
  "id": "bluemapofflineplayermarkers",
  "version": "${version}",
  "name": "BlueMapOfflinePlayerMarkers",
  "description": "Adds markers where players have logged off to BlueMap",
  "authors": [
    "TechnicJelle",
    "Mark-255",
    "Blue",
    "Seercat3160",
    "Syorito Hatsuki"
  ],
  "contact": {
    "homepage": "https://github.com/TechnicJelle/BlueMapOfflinePlayerMarkers"
  },
  "license": "MIT Licence",
  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "com.technicjelle.bluemapofflineplayermarkers.BlueMapOfflinePlayerMarkers"
      }
    ]
